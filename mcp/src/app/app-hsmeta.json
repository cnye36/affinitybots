{
  "uid": "app-affinitybots",
  "type": "app",
  "config": {
    "description": "An app to connect AffinityBots to Hubspot through MCP",
    "name": "affinitybots-Application",
    "distribution": "marketplace",
    "isUserLevel": true,
    "auth": {
      "type": "oauth",
      "redirectUrls": [
        "http://localhost:3000/api/hubspot/oauth/callback",
        "https://affinitybots.com/api/hubspot/oauth/callback",
        "https://www.affinitybots.com/api/hubspot/oauth/callback"
      ],
      "requiredScopes": [
        "crm.objects.contacts.read",
        "crm.objects.companies.read"
        
      ],
      
      "optionalScopes": [],
      "conditionallyRequiredScopes": []
    },
    "permittedUrls": {
      "fetch": [
        "https://api.hubapi.com"
      ],
      "iframe": [],
      "img": []
    },
    "support": {
      "supportEmail": "support@affinitybots.com",
      "documentationUrl": "https://affinitybots.com/docs",
      "supportUrl": "https://affinitybots.com",
      "supportPhone": "+13852982222"
    }
  }
}